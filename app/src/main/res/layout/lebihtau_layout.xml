<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:padding="18dp"
    android:background="@color/white"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/pendapatan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:text="Pendapatan Lain"/>

    <TextView

        android:layout_width="wrap_content"
        android:layout_marginTop="10dp"
        android:textSize="17sp"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:text="Uang yang masuk ke laci atau kasir rekening yang menambah profit / laba bisnis kamu"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18dp"
        android:layout_marginVertical="10dp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:text="Contoh"/>
    <TextView

        android:layout_width="wrap_content"

        android:textSize="17sp"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:text="Menjual kardus bekas ke loak"/>


    <TextView
        android:layout_width="match_parent"
        android:layout_marginVertical="10sp"
        android:background="#CFCACA"
        android:layout_height="2dp"/>

    <TextView
        android:id="@+id/nonpendapat"
        android:layout_width="wrap_content"
        android:textStyle="bold"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:textColor="@color/black"

        android:text="Non Pendapatan"/>

    <TextView

        android:layout_width="wrap_content"
        android:layout_marginTop="10dp"
        android:textSize="17sp"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:text="Uang yang masuk ke laci atau kasir rekening yang tidak profit / laba bisnis kamu"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18dp"
        android:layout_marginVertical="10dp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:text="Contoh"/>
    <TextView

        android:layout_width="wrap_content"

        android:textSize="17sp"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:text="Modal awal untuk kembalian pelanggan"/>

</LinearLayout>